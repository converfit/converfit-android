<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.citious.converfit.Actividades.Details.VisorPDFActivity">

    <include
        android:id="@+id/activity_visor_pdf_tool_bar"
        layout="@layout/tool_bar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"/>

    <WebView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/webView_activity_visor_pdf"
        android:layout_below="@+id/activity_visor_pdf_tool_bar"/>

</RelativeLayout>
